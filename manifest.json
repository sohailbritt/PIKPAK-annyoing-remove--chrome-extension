{
    "manifest_version": 3,
    "name": "Remove Tooltip Reference Layer",
    "version": "1.0",
    "description": "Removes the 'introjs-tooltipReferenceLayer' class from mypikpak.com pages.",
    "permissions": [
      "activeTab"
    ],
    "content_scripts": [
      {
        "matches": ["*://*.mypikpak.com/*"],
        "js": ["content.js"]
      }
    ],
    "icons": {
      "48": "icon.png"
    }
  }
